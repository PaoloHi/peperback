<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, user-scalable=1.0">
     <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v6.1.1/css/all.css"
          integrity="sha384-/frq1SRXYH/bSyou/HUp/hib7RVN1TawQYja658FEOodR/FQBKVqT9Ol+Oz3Olq5" crossorigin="anonymous">
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css" rel="stylesheet">
     <link rel="icon" type="image/png" sizes="16x16" href="~/img/imagen-logo/logo_oficial.png" />
   <link rel="stylesheet" href="css/style1.css">
     <script type="text/javascript"
            src="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="~/css/slick.css" />
    <link rel="stylesheet" href="~/css/slick-theme.css" />
    <link rel="icon" type="image/png" sizes="16x16" href="img/logo_oficial.png">
    
    
</head>
<body onload="initialize()">
    <!--AQUI COMIENZA EL NAVBAR-->
    <header class="header sticky-top">
        <nav class="navbar navbar-expand-lg navbar-dark p-2 sticky-top activar">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav col-lg-6 ">
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Ecommerce" asp-action="Propiedades">Propiedades</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Ecommerce" asp-action="PlanesEcommerce">Planes</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Nosotros</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" >Ayuda</a>
                        </li>
                    </ul>

                    <ul class="navbar-nav col-lg-4 d-lg-block">
                        <li class="nav-item">
                            <a class="" href="">
                                <img src="~/img/imagenes__e-commerce/logo_oficial_blanco.png" class="img-fluid image-size d-none d-lg-flex" loading="lazy" alt="">
                            </a>
                        </li>

                    </ul>

                    <ul class="navbar-nav col-lg-2">
                        <li class="nav-item mb-2">

                            <a class="nav-link rounded me-lg-3 mt-1 text-start d-lg-flex  align-items-center"
                              asp-controller="Ecommerce" asp-action="PlanesEcommerce"
                               style="background-color:#ff7f40; border-radius: 5px!important; height: 30px; width:6em;">

                                <h6 class="text-light mt-lg-2 mb-2 ml-2 ml-lg-0">Afiliarme</h6>
                            </a>


                        </li>
                        <li class="nav-item">

                            <a class="nav-link rounded ml-2 " asp-controller="Ecommerce" asp-action="Contacto">
                                <i class="fa-solid fa-phone size-icon-navbar"></i>
                            </a>


                        </li>
                        <li class="nav-item ">

                            <a class="nav-link rounded ml-2 " asp-controller="Cuentas" asp-action="Acceso">
                                <i class="fa-solid fa-user size-icon-navbar"></i>
                            </a>

                        </li>


                    </ul>



                </div>
            </div>
        </nav>
    </header>
    <!--Aquí termina el NAVBAR-->
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    <!--AQUI VA EL FOOTER-->
    <!--Aqui comienza el footer-->
    <footer class="text-white text-center text-lg-start main_backg pb-lg-3 pb-md-3">
        <!-- Grid container -->
        <div class="container">
            <!--Grid row-->
            <div class="row ">
                <!--Grid column-->
                <div class="col-lg-4 col-md-6   mb-md-0 mb-4 d-grid align-items-center  mt-lg-2">




                    <div class="d-inline-flex mt-lg-2 align-items-center align-text-center ml-2">
                        <i class="fas fa-envelope size-icons-footer-2 mt-2"></i>
                        <h6 class="ml-2 size-text-footer mt-3">contacto@hotmail.com</h6>

                    </div>
                    <br />
                    <div class="d-inline-flex align-items-center align-text-center mr-lg-5 ">
                        <i class="fas fa-phone size-icons-footer-2"></i>
                        <h6 class="ml-2 size-text-footer mt-2">55 4532 4598</h6>
                    </div>


                </div>

                <!--Grid column-->
                <div class="col-lg-4 col-md-12  mb-md-0 ms-lg-4 d-grid align-items-end mt-3 justify-content-center">

                    <div class="mb-2 mt-2 d-flex justify-content-center ">

                        <!-- Facebook -->
                        <a type="button" class="ml-3 icon"><i class=" fab icon-background fa-facebook-f size-icons-footer "></i></a>

                        <!-- Instagram -->
                        <a type="button" class="ml-3 icon"><i class="fab icon-background fa-instagram size-icons-footer"></i></a>
                        <!-- Whatsapp -->
                        <a type="button" class="icon ml-3"><i class="fa-brands fa-whatsapp icon-background size-icons-footer me-4"></i></a>
                        <!-- Linkedin -->
                        <a type="button" class="icon ml-3"><i class="fa-brands fa-linkedin-in icon-background size-icons-footer"></i></a>
                    </div>

                    <div class=" d-flex justify-content-center ">
                        <a href="" class="size-text-footer-copyr ml-lg-3">&copy;2022 Corporativo Valle Oriente</a>
                    </div>

                </div>
                <!--Grid column-->
                <div class="col-lg-3 col-md-12 mb-4 mb-lg-2 b-md-0 ml-lg-5 d-flex align-items-center justify-content-lg-end mt-lg-4">

                    <div class="">
                        <a href="" class="size-text-footer justify-content-end d-grid ml-4">Aviso de privacidad</a>
                        <br />
                        <a href="" class="color-link-footer size-text-footer">Términos y condiciones</a>


                    </div>


                </div>



            </div>
            <!--Grid row-->
        </div>



    </footer>

        <!--AQUI TERMINO EL FOOTER-->
       
         

    <script src="https://code.jquery.com/jquery-3.6.1.js"integrity="sha256-3zlB5s2uwoUzrXK3BT7AX3FyvojsraNFxCc2vC/7pNI="crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"></script>
                  <script src="~/js/googleMapsPrincipal.js"></script>
    <script async
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyALRWuVx6gxkmUDTEJNiigxwGBgcYBX00Y&libraries=places">
    </script>
    
     <script type="text/javascript">
            $('.slideshow').slick({
  dots: true,
  infinite: true,
  speed: 600,
  slidesToShow: 4,
  slidesToScroll: 4,
  autoplay: true,
  responsive: [
    {
      breakpoint: 1024,
      settings: {
        slidesToShow: 3,
        speed: 600,
        slidesToScroll: 3,
        infinite: true,
        autoplay: true,
        dots: true
      }
    },
    {
      breakpoint: 600,
      settings: {
        slidesToShow: 2,
        speed: 600,
        autoplay: true,
        slidesToScroll: 2
      }
    },
    {
      breakpoint: 480,
      settings: {
        slidesToShow: 1,
        speed: 600,
        autoplay: true,
        slidesToScroll: 1
      }
    }
    // You can unslick at a given breakpoint now by adding:
    // settings: "unslick"
    // instead of a settings object
  ]
});
           </script>
    

   
</body>
</html>



  
